/* pages/my-plan/components/header/header.wxss */
.container {
  display: flex;
  padding: 0 30rpx 10rpx;
  justify-content: space-between;
}

.title {
  display: flex;
}

.title .txt {
  align-self: flex-end;
  font-size: 60rpx;
  font-weight: 700;
}

.bubble {
  align-self: center;
  margin-left: 20rpx;
  color: #fff;
  padding: 14rpx;
  font-weight: 700;
  font-size: 22rpx;
  border-radius: 50px 50px 50px 10px;
}